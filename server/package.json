    "concurrently": "^6.0.0",
    "nodemon": "^2.0.7"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build-server": "tsc",
    "build": "tsc && cd ../svelte/ && npm run build",
