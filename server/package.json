{
  "name": "public-planner",
  "version": "1.0.0",
  "description": "A anonymous public planner for people or puppys",
  "main": "index.js",
  "dependencies": {
    "apollo-server-express": "^2.21.1",
    "compression": "^1.7.4",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "graphql": "^15.5.0",
    "mongoose": "^5.12.1",
    "path": "^0.12.7"
  },
  "devDependencies": {
    "@types/mongoose": "^5.10.3",
    "@types/compression": "^1.7.0",
    "@types/express": "^4.17.11",
